[user]
	name = Keenan Tullis
	email = keenanjt@umich.edu
[color]
	diff = auto
	status = auto
    branch = auto
[alias]
  lg = log --graph --abbrev-commit --decorate --date=relative --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(bold blue)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
  logdoga = log --decorate --oneline --graph --all
  s = status
  d = diff
[filter "lfs"]
	required = true
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
